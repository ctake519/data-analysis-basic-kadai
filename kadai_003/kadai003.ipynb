        "import numpy as np\n",
        "\n",
        "A = np.array([[0, 1],\n",
        "              [2, 3],\n",
        "              [4, 5]])\n",
        "\n",
        "B = np.array([[0, 1, 2 ,3],\n",
        "              [4, 5, 6, 7]])\n",
        "\n",
        "C = np.dot(A, B)\n",
        "\n",
        "max_value = np.max(C)\n",
        "\n",
        "print(\"行列の積:\",C)\n",
        "print(\"行列の要素の最大値:\",max_value)"
      ],
